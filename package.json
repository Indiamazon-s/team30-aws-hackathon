    "@aws-sdk/client-transcribe": "^3.882.0",
    "@aws-sdk/client-translate": "^3.882.0",
    "@aws-sdk/lib-dynamodb": "^3.882.0",
    "@types/js-cookie": "^3.0.6",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.2",
    "js-cookie": "^3.0.5",
    "next": "^14.2.32",
    "oidc-client-ts": "^3.3.0",
    "postcss": "^8.4.31",
    "react": "^18",
    "react-dom": "^18",
